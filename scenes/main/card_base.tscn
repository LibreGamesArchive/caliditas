code = "shader_type canvas_item;

uniform float offset = 0.0;
uniform sampler2D gradient;

void fragment()
{
	vec2 uv = UV+vec2(0,offset);
	COLOR = texture(TEXTURE,UV);
	COLOR.rgb += COLOR.a*texture(gradient,uv).g;
}"

