code = "shader_type canvas_item;

uniform float rot = 0.0;
uniform sampler2D cone;
uniform sampler2D colors;

void fragment()
{
	vec2 uv1 = (UV-vec2(0.5))*mat2(vec2(cos(rot),sin(rot)),vec2(-sin(rot),cos(rot)))+vec2(0.5);
	vec2 uv2 = (UV-vec2(0.5,0.5))*mat2(vec2(cos(-rot),sin(-rot)),vec2(-sin(-rot),cos(-rot)))+vec2(0.5,0.5);
	float hl = texture(cone,uv1).g;
	float blend = 0.25+0.5*hl;
	COLOR = texture(TEXTURE,UV);
	COLOR.rgb *= 1.0-blend+blend*texture(colors,uv2).rgb;
	COLOR.rgb += COLOR.a*hl;
}"

