[sub_resource type="Shader" id=1]

code = "shader_type canvas_item;

uniform sampler2D mask;

void fragment()
{
	// Mask the corners via mask texture.
	// Scale UV coordinates to fit the 'keep aspect covered' scaling.
	float ratio = float(textureSize(mask,0).x)/float(textureSize(mask,0).y)*float(textureSize(TEXTURE,0).y)/float(textureSize(TEXTURE,0).x);
	vec2 uv = UV;
	if (ratio<1.0)
		uv = (UV-vec2(0.5))*vec2(1.0/ratio,1.0)+vec2(0.5);
	else
		uv = (UV-vec2(0.5))*vec2(1.0,ratio)+vec2(0.5);
	COLOR = texture(TEXTURE,UV);
	COLOR.a *= texture(mask,uv).g;
}
"

